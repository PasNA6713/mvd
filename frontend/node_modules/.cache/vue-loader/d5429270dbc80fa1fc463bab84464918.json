 <v-text-field\n            v-model=\"password\"\n            label=\"Пароль\"\n            type=\"password\"\n            required\n          ></v-text-field>\n\n          <v-btn\n            color=\"#27646A\"\n            style=\"color: white\"\n            @click=\"signIn\"\n            class=\"sign-in-form__button\"\n            block\n            large\n          >\n            Войти\n          </v-btn>\n          <br>\n          <v-alert\n            type=\"warning\"\n            v-if=\"isFalseLogin\"\n            color=\"#ff0000\"\n          >Некорректно введены данные!</v-alert>\n        </v-card>\n      </v-col> \n    </v-row>\n  </div>\n</template>\n\n<script>\n  export default {\n    data: () => ({\n      isFalseLogin: false,\n      password: \"\",\n      username: \"\"\n    }),\n    \n    methods: {\n      async signIn() {\n        try {\n          await axios.post(`${this.$store.state.backendUrl}/auth/token/login/`, \n          {\n              'Content-Type': 'application/json',\n              'username': this.username,\n              'password': this.password\n          }\n        ).then(response => {\n              this.$store.dispatch('setToken', response.data.auth_token) \n              this.$router.push({ name: 'home' })\n          })\n        } catch (Error) {\n          this.isFalseLogin = true\n        }\n      }\n    }\n  }\n</script>\n\n<style lang=\"scss\" scoped>\n@import '@/styles/common.scss';\n@import '@/styles/views/signIn.scss';\n</style>"]}]}